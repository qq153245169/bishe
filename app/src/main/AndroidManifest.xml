    <activity android:name=".module.selectcar.SelectCarActivity"/>
    <activity android:name=".activity.CommonActivity"/>
        <activity android:name=".view.hotcity.CityListActivity"/>
    <activity android:name=".module.register.LoginActivity" />
    <activity android:name=".module.register.RegisterActivity" />

    <!--百度地图service-->
    <service android:name="com.baidu.location.f" android:enabled="true" android:process=":remote">
    </service>
    <meta-data
        android:name="com.baidu.lbsapi.API_KEY"
        android:value="1Bf1F5OyN5OG9RdlEmgsNBuOkygqwbkU" />       //key:百度地图SDK开发者申请的Key
    </application>
